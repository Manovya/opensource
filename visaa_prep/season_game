def getseason(N):
    if N == 3 or N == 4 or N == 5:
        return "Spring"
    elif N == 6 or N == 7 or N == 8:
        return "Summer"
    elif N == 9 or N == 10 or N == 11:
        return "Autumn"
    elif N == 12 or N == 1 or N == 2:
        return "Winter"
    else:
        return "Invalid"
N = int(input())
print(getseason(N))
